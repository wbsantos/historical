package com.wallpapers;

import com.avatarbatterywallpaper.AvatarWallpaperService;
import com.avatarbatterywallpaper.R;

public class KorraPurifyingCity extends AvatarWallpaperService 
{

	@Override
	public int getWallpaperId(int i) {
		switch (i) {
		case 0:
			return R.drawable.adultaang000;
		case 1:
			return R.drawable.adultaang005;
		case 2:
			return R.drawable.adultaang010;
		case 3:
			return R.drawable.adultaang015;
		case 4:
			return R.drawable.adultaang020;
		case 5:
			return R.drawable.adultaang025;
		case 6:
			return R.drawable.adultaang030;
		case 7:
			return R.drawable.adultaang035;
		case 8:
			return R.drawable.adultaang040;
		case 9:
			return R.drawable.adultaang045;
		case 10:
			return R.drawable.adultaang050;
		case 11:
			return R.drawable.adultaang055;
		case 12:
			return R.drawable.adultaang060;
		case 13:
			return R.drawable.adultaang065;
		case 14:
			return R.drawable.adultaang070;
		case 15:
			return R.drawable.adultaang075;
		case 16:
			return R.drawable.adultaang080;
		case 17:
			return R.drawable.adultaang085;
		case 18:
			return R.drawable.adultaang090;
		case 19:
			return R.drawable.adultaang095;
		case 20:
			return R.drawable.adultaang100;
		default:
			return R.drawable.adultaang000;
		}
	}

}

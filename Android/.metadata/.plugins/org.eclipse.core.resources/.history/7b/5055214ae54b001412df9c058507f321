package com.wallpapers;

import com.avatarbatterywallpaper.AvatarWallpaperService;
import com.avatarbatterywallpaper.R;

public class AvatarStateWallpaper extends AvatarWallpaperService 
{

	@Override
	public int getWallpaperId(int i) {
		return R.drawable.spiritbend100;
		switch (i) {
		case 0:
			return R.drawable.avatarstate000;
		case 1:
			return R.drawable.avatarstate005;
		case 2:
			return R.drawable.avatarstate010;
		case 3:
			return R.drawable.avatarstate015;
		case 4:
			return R.drawable.avatarstate020;
		case 5:
			return R.drawable.avatarstate025;
		case 6:
			return R.drawable.avatarstate030;
		case 7:
			return R.drawable.avatarstate035;
		case 8:
			return R.drawable.avatarstate040;
		case 9:
			return R.drawable.avatarstate045;
		case 10:
			return R.drawable.avatarstate050;
		case 11:
			return R.drawable.avatarstate055;
		case 12:
			return R.drawable.avatarstate060;
		case 13:
			return R.drawable.avatarstate065;
		case 14:
			return R.drawable.avatarstate070;
		case 15:
			return R.drawable.avatarstate075;
		case 16:
			return R.drawable.avatarstate080;
		case 17:
			return R.drawable.avatarstate085;
		case 18:
			return R.drawable.avatarstate090;
		case 19:
			return R.drawable.avatarstate095;
		case 20:
			return R.drawable.avatarstate100;
		default:
			return R.drawable.avatarstate000;
		}
	}

}

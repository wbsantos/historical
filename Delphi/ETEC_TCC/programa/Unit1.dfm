  object ADOpesq_hist: TADOQuery
    Active = True
    Connection = DMeclipse.ADOCBD
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'codi'
        Attributes = [paNullable]
        DataType = ftWideString
        NumericScale = 255
        Precision = 255
        Size = 510
        Value = Null
      end>
    SQL.Strings = (
      'SELECT tb_aluno.codigo_alu,'
      '  tb_aluno.nome_alu, '
      '  tb_curso.nome_cur, '
      '  tb_historico.codigo_hist,'
      '  tb_historico.nfaltas, '
      '  tb_historico.status_alu, '
      '  tb_historico.obs_alu, '
      '  tb_historico.mediafinal_alu, '
      '  tb_turma.dt_ini, '
      '  tb_turma.nome_tur, '
      '  tb_turma.dt_fin, '
      '  tb_turma.dt_prev, '
      '  tb_turma.horario_tur'
      
        'FROM (tb_curso INNER JOIN tb_turma ON tb_curso.codigo_cur = tb_t' +
        'urma.codigo_cur) INNER JOIN (tb_aluno INNER JOIN tb_historico ON' +
        ' tb_aluno.codigo_alu = tb_historico.codigo_alu) ON tb_turma.codi' +
        'go_tur = tb_historico.codigo_tur'
      'where tb_aluno.codigo_alu = :codi')
    Left = 336
    Top = 72
